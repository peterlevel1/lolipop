{
  "name": "lolipop",
  "version": "1.0.0",
  "description": "a nodejs server",
  "private": true,
  "dependencies": {
    "debug": "^4.0.1",
    "egg": "^2.2.1",
    "egg-mysql": "^3.0.0",
    "egg-session": "^3.1.0",
    "egg-validate": "^1.0.0"
  },
  "devDependencies": {
    "autod": "^3.0.1",
    "autod-egg": "^1.0.0",
    "egg-bin": "^4.3.5",
    "egg-ci": "^1.8.0",
    "egg-mock": "^3.14.0",
    "eslint": "^4.11.0",
    "eslint-config-egg": "^6.0.0",
    "webstorm-disable-index": "^1.2.0"
  },
  "engines": {
    "node": ">=8.9.0"
  },
  "scripts": {
    "start": "egg-scripts start --daemon --title=egg-server-lolipop",
    "stop": "egg-scripts stop --title=egg-server-lolipop",
    "dev": "DEBUG=app:*,egg-security:context egg-bin dev --port=3000",
    "debug": "egg-bin debug --port=3000",
    "test": "npm run lint -- --fix && npm run test-local",
    "test-local": "egg-bin test",
    "cov": "egg-bin cov",
    "lint": "eslint .",
    "ci": "npm run lint && npm run cov",
    "autod": "autod"
  },
  "ci": {
    "version": "8"
  },
  "repository": {
    "type": "git",
    "url": ""
  },
  "author": "cc",
  "license": "MIT"
}